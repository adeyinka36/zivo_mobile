{
  "expo": {
    "name": "zivo_mobile",
    "slug": "zivo_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "extra": {
      "devHost": "192.168.1.159",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6e27c21c-41b3-40f9-9bfd-1e5dce873c02"
      }
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "scheme": "zivo",
    "plugins": [
      "expo-router",
      "expo-video",
      "react-native-video"
    ]
  }
}
